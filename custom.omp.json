
{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#1637cc",
          "properties": {
            "prefix": "",
            "text": "\u279C"
          }
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "#1f10f0",
          "properties": {
            "display_host": false
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#0fec22"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#cc4709",
          "properties": {
            "branch_icon": "",
            "display_status": true,
            "prefix": "<#5FAAE8>git:(</>",
            "postfix": "<#5FAAE8>)</>",
            "display_upstream_icon": true
          }
        }        
      ]
    }
  ],
  "final_space": true
}
